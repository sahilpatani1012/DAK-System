          <label for="colFormLabelLg" class="col-sm-2 col-form-label col-form-label-lg">Establishment :</label>
          <div class="col-sm-10">
            <input required name="section1DakCount" min="0" type="number" class="form-control form-control-lg" id="colFormLabelLg"
              placeholder="Enter the number" />
          </div>
        </div>
        <div class="row mb-3">
          <label for="colFormLabelLg" class="col-sm-2 col-form-label col-form-label-lg">Justice :</label>
          <div class="col-sm-10">
            <input required name="section2DakCount" min="0" type="number" class="form-control form-control-lg" id="colFormLabelLg"
              placeholder="Enter the number" />
          </div>
        </div>
        <div class="row mb-3">
          <label for="colFormLabelLg" class="col-sm-2 col-form-label col-form-label-lg">Revenue :</label>
          <div class="col-sm-10">
            <input required name="section3DakCount" min="0" type="number" class="form-control form-control-lg" id="colFormLabelLg"
              placeholder="Enter the number" />
          </div>
        </div>
        <div class="row mb-3">
