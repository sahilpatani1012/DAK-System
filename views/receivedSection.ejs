          <div class="col-sm-10">
            <input required name="section24DakCount" min="0" type="number" class="form-control form-control-lg" id="colFormLabelLg"
              placeholder="Enter the number" />
          </div>
        </div>
        <div class="mt-3 mb-3 text-center">
          <button type="submit" class="btn btn-outline-success">
